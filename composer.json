{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "vova07/yii2-imperavi-widget": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-widget-colorinput": "*",
        "la-haute-societe/yii2-save-relations-behavior": "*",
        "yii-dream-team/yii2-upload-behavior": "*",
        "webmozart/assert": "^1.2",
        "yiisoft/yii2-authclient": "^2.1",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-field-range": "*",
        "elasticsearch/elasticsearch": "^5.2",
        "fishvision/yii2-migrate": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "flow/jsonpath": "^0.3.4",
        "filsh/yii2-oauth2-server": "^2.0",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-queue": "^2.0@dev",
        "yiisoft/yii2-imagine": "^2.0@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.4.0",
        "phpunit/phpunit": "~6.5.5",
        "codeception/verify": "~1.0.0",
        "zircote/swagger-php": "^2.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "https://github.com/Nikkoz/news"
        }
    ],
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
